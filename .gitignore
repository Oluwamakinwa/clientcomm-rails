.DS_Store

# no bundler config
/.bundle

# no logfiles or tempfiles
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
/coverage/*
/results/*

# Terraform
.terraform/
*.tfstate
*.tfstate.backup

# JetBrains RubyMine
.idea/*

# no byebug command history file
.byebug_history

# no private environment variables
.env
.env-*

# no private keys
*.pem

tags

# Paperclip
/public/system/*

.floo

# VIM
*.swp

# Node.js
node_modules

#Ansible
*.retry
